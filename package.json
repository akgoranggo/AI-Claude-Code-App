{
  "name": "web-app-template",
  "version": "1.0.0",
  "description": "Enterprise web application template with React, Express, and Azure Databricks Lakebase",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "node launch-both.js",
    "dev:client": "vite --port 3000",
    "dev:server": "NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "db:generate": "npx tsx scripts/drizzle-with-auth.ts generate",
    "db:migrate": "npx tsx scripts/drizzle-with-auth.ts migrate",
    "db:push": "npx tsx scripts/drizzle-with-auth.ts push",
    "db:studio": "npx tsx scripts/drizzle-with-auth.ts studio",
    "db:seed": "npx tsx scripts/seed-database.ts",
    "db:seed:fresh": "npx tsx scripts/seed-database.ts --drop-first",
    "db:create": "npx tsx scripts/seed-database.ts --create-schema",
    "prepare": "husky"
  },
